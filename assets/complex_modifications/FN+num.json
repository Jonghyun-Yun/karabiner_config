{
  "title": "FN-mode",
  "maintainers": ["jonghyun-yun"],
  "rules": [
    {
      "description": "FN+num",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "period"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "0"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "8",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "1"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "2"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "3"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "4"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "5"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "6"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },{
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    }
  ]
}
